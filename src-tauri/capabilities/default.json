{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "shell:allow-open",
    "fs:default",
    "fs:allow-app-read",
    {
      "identifier": "fs:allow-app-read",
      "allow": [
        "$HOME/**"
      ]
    },
    "log:default",
    "dialog:default"
  ]
}